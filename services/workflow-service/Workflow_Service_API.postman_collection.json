				{
					"name": "Initialize Approval Tracking",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 201) {",
									"    var jsonData = pm.response.json();",
									"    pm.collectionVariables.set(\"approvalTrackingId\", jsonData.id);",
									"    console.log(\"Approval Tracking ID:\", jsonData.id);",
									"    console.log(\"Assigned User ID:\", jsonData.assignedUserId);",
